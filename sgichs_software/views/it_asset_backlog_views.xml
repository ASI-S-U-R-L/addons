<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="it_asset_backlog_view_form_inherit_software" model="ir.ui.view">
        <field name="name">it.asset.backlog.form.inherit.software</field>
        <field name="model">it.asset.backlog</field>
        <field name="inherit_id" ref="sgichs_hardware.it_asset_backlog_view_form_inherit_hardware"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Software Detectado" name="software">
                    <field name="software_ids" readonly="1"/>
                </page>
            </xpath>
        </field>
    </record>
</odoo>