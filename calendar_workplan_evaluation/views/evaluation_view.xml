<odoo>
<record id="view_calendar_workplan_evaluation_form" model="ir.ui.view">
    <field name="name">calendar.workplan.evaluation.form</field>
    <field name="model">calendar_workplan.evaluation</field>
    <field name="arch" type="xml">
        <form string="Evaluación IA">
            <header>
                <button name="%(calendar_workplan_evaluation.action_report_calendar_workplan_evaluation)d"
                        string="Imprimir Evaluación"
                        type="action"
                        class="btn-primary"
                        context="{'active_ids': [active_id]}"/>
            </header>
            <group>
                <field name="plan_id" readonly="1"/>
                <field name="evaluation_date" readonly="1"/>
                <field name="compliance_percentage" readonly="1"/>
                <field name="quantitative_score" readonly="1"/>
                <field name="qualitative_analysis" readonly="1"/>
            </group>
        </form>
    </field>
</record>
</odoo>
