        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Secuencia para entregas/devoluciones de tarjetas -->
    <record id="sequence_fuel_card_delivery" model="ir.sequence">
        <field name="name">Secuencia de Entregas/Devoluciones de Tarjeta</field>
        <field name="code">fuel.card.delivery</field>
        <field name="prefix">DEL</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Secuencia para tickets de combustible -->
    <record id="sequence_fuel_ticket" model="ir.sequence">
        <field name="name">Secuencia de Tickets de Combustible</field>
        <field name="code">fuel.ticket</field>
