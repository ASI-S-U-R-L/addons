        <field name="code">fuel.unassigned</field>
        <field name="prefix">UNA</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Secuencia para facturas de combustible -->
    <record id="sequence_fuel_invoice" model="ir.sequence">
        <field name="name">Secuencia de Facturas de Combustible</field>
        <field name="code">fuel.invoice</field>
