<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Secuencia para titulares de tarjetas -->
    <record id="sequence_fuel_card_holder" model="ir.sequence">
        <field name="name">Secuencia de Titulares de Tarjeta</field>
        <field name="code">fuel.card.holder</field>
        <field name="prefix">TCH</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Secuencia para proveedores de combustible -->
    <record id="sequence_fuel_supplier" model="ir.sequence">
        <field name="name">Secuencia de Proveedores de Combustible</field>
        <field name="code">fuel.supplier</field>
        <field name="prefix">SUP</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Secuencia para generadores de energía -->
    <record id="sequence_fuel_power_generator" model="ir.sequence">
        <field name="name">Secuencia de Generadores de Energía</field>
        <field name="code">fuel.power.generator</field>
        <field name="prefix">GEN</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Secuencia para conductores -->
    <record id="sequence_fuel_driver" model="ir.sequence">
        <field name="name">Secuencia de Conductores</field>
        <field name="code">fuel.driver</field>
        <field name="prefix">DRV</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
