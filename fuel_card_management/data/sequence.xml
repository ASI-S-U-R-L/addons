    <record id="sequence_fuel_balance_transfer" model="ir.sequence">
        <field name="name">Secuencia de Traspasos de Saldo</field>
        <field name="code">fuel.balance.transfer</field>
        <field name="prefix">TRF</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Secuencia para planes de combustible -->
    <record id="sequence_fuel_plan" model="ir.sequence">
        <field name="name">Secuencia de Planes de Combustible</field>
        <field name="code">fuel.plan</field>
        <field name="prefix">PLAN</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
